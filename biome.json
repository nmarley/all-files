{
  "formatter": {
    "enabled": true,
    "indentStyle": "space",
    "indentWidth": 4,
    "lineWidth": 80
  },
  "javascript": {
    "formatter": {
        "enabled": true,
        "quoteStyle": "single"
    }
  },
  "json": {
    "formatter": {
        "enabled": false
    },
    "linter": {
        "enabled": false
    }
  },
  "linter": {
    "enabled": true,
    "rules": {
      "style": {
        "noNonNullAssertion": "off",
        "useImportType": "off"
      }
    }
  }
}
